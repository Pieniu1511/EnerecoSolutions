<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Enereko Solutions</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@400;700&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/4e208eaa12.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="dist/css/style.min.css">
</head>

<body>

    <!-- @include 'nav.kit' -->
    <div class="hero">
        <div class="hero-top">
            <p>Skontaktuj się z nami</p>
        </div>
    </div>
    <div class="contact">
        <div class="contact-logo"><img src="dist/img/LogoEnereko2 (1)-1.png" alt="Logo firmy Enereko Solutions"></div>
        <form action="" class="contact-form wrapper">
            <label for="fname" class="contact-form-label">Imię i nazwisko: <i class="fa-solid fa-id-badge"></i></label>
            <input type="text" id="fname" class="contact-form-input">
            <label for="fadress" class="contact-form-label">Adres: <i class="fa-solid fa-location-dot"></i></label>
            <input type="text" id="fadress" class="contact-form-input">
            <label for="fphone" class="contact-form-label">Numer telefonu: <i class="fa-solid fa-phone"></i></label>
            <input type="tel" id="fphone" class="contact-form-input">
            <label for="femail" class="contact-form-label">Email: <i class="fa-solid fa-envelope"></i></label>
            <input type="email" id="femail" class="contact-form-input">
            <label for="fchoice" class="contact-form-label">Temat:</label>
            <select name="" id="fchoice" class="contact-form-input">
                <option value="0" class="contact-form-option">Wybierz</option>
                <option value="1" class="contact-form-option">Instalacje fotowoltaiczne</option>
                <option value="2" class="contact-form-option">Pompy ciepła</option>
                <option value="3" class="contact-form-option">Broker energii</option>
                <option value="4" class="contact-form-option">Inny... (napisz)</option>
            </select>
            <label for="fmessage"></label>
            <textarea name="" id="fmessage" class="contact-form-textarea"></textarea>
        </form>
    </div>

    <!-- @include 'footer.kit' -->

    <script src="dist/js/script.min.js"></script>
</body>

</html>